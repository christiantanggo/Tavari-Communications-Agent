{
  "name": "tavari-ai-phone-agent",
  "version": "1.0.0",
  "description": "AI Phone Agent - Self-serve AI phone answering service",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "dev:clean": "powershell -ExecutionPolicy Bypass -File ./start-dev.ps1",
    "kill-port": "powershell -ExecutionPolicy Bypass -File ./kill-port.ps1",
    "lint": "eslint .",
    "format": "prettier --write .",
    "migrate": "node scripts/migrate.js",
    "test:vapi": "node scripts/test-vapi-connection.js",
    "test:phone": "node scripts/test-phone-provisioning.js",
    "test:key": "node scripts/test-vapi-key-direct.js",
    "test:phone-direct": "node scripts/test-vapi-phone-direct.js",
    "verify:vapi": "node scripts/verify-vapi-setup.js",
    "check:mapping": "node scripts/check-business-phone-mapping.js",
    "check:assistant": "node scripts/check-assistant-details.js",
    "fix:webhook": "node scripts/fix-assistant-webhook.js",
    "fix:transcriber": "node scripts/fix-assistant-transcriber.js",
    "test:webhook": "node scripts/test-vapi-webhook.js",
    "diagnose:webhook": "node scripts/diagnose-webhook.js",
    "create-admin": "node scripts/create-admin-user.js",
    "check:phones": "node scripts/check-and-fix-phone-numbers.js",
    "setup:helcim": "node scripts/setup-helcim-products.js",
    "test:helcim": "node scripts/test-helcim-connection.js",
    "test:helcim-simple": "node scripts/test-helcim-simple.js",
    "test:verification": "node scripts/test-telnyx-verification-api.js",
    "test:toll-free": "node scripts/test-toll-free-verification.js",
    "test:payment": "node scripts/test-payment-flow.js",
    "test:payment:auto": "node scripts/test-payment-flow-automated.js",
    "check:payment": "node scripts/check-payment-status.js",
    "verify:webhook": "node scripts/verify-webhook-received.js",
    "list:sessions": "node scripts/list-recent-checkout-sessions.js",
    "diagnose:payment": "node scripts/diagnose-payment-issue.js",
    "process:checkout": "node scripts/manual-process-checkout.js",
    "check:today": "node scripts/check-today-sessions.js",
    "check:latest": "node scripts/check-latest-session-status.js",
    "check:all": "node scripts/check-all-recent-sessions.js",
    "check:packages": "node scripts/check-business-packages.js",
    "check:failed": "node scripts/check-failed-payments.js",
    "diagnose:flow": "node scripts/diagnose-payment-flow.js",
    "check:webhook": "node scripts/check-stripe-webhook-config.js",
    "fix:minutes": "node scripts/fix-business-minutes.js",
    "auto-commit": "node -e \"require('child_process').execSync('git add -A && git commit -m \\\"Auto-commit: Update files\\\" && git push origin main', {stdio: 'inherit'})\"",
    "deploy": "npm run auto-commit"
  },
  "keywords": [
    "ai",
    "phone",
    "voximplant",
    "openai",
    "realtime"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.952.0",
    "@aws-sdk/client-ses": "^3.952.0",
    "@sentry/node": "^7.120.4",
    "@supabase/supabase-js": "^2.87.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.12.31",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "pdfkit": "^0.14.0",
    "stripe": "^20.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "prettier": "^3.1.1"
  }
}
