{
  "name": "tavari-ai-phone-agent",
  "version": "1.0.0",
  "description": "AI Phone Agent - Self-serve AI phone answering service",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "migrate": "node scripts/migrate.js",
    "test:vapi": "node scripts/test-vapi-connection.js",
    "test:phone": "node scripts/test-phone-provisioning.js",
    "test:key": "node scripts/test-vapi-key-direct.js",
    "test:phone-direct": "node scripts/test-vapi-phone-direct.js",
    "verify:vapi": "node scripts/verify-vapi-setup.js",
    "check:mapping": "node scripts/check-business-phone-mapping.js",
    "check:assistant": "node scripts/check-assistant-details.js",
    "fix:webhook": "node scripts/fix-assistant-webhook.js",
    "fix:transcriber": "node scripts/fix-assistant-transcriber.js",
    "test:webhook": "node scripts/test-vapi-webhook.js",
    "diagnose:webhook": "node scripts/diagnose-webhook.js",
    "auto-commit": "node -e \"require('child_process').execSync('git add -A && git commit -m \\\"Auto-commit: Update files\\\" && git push origin main', {stdio: 'inherit'})\"",
    "deploy": "npm run auto-commit"
  },
  "keywords": [
    "ai",
    "phone",
    "voximplant",
    "openai",
    "realtime"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.952.0",
    "@aws-sdk/client-ses": "^3.952.0",
    "@sentry/node": "^7.120.4",
    "@supabase/supabase-js": "^2.87.1",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "handlebars": "^4.7.8",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "libphonenumber-js": "^1.12.31",
    "node-fetch": "^3.3.2",
    "pdfkit": "^0.14.0",
    "stripe": "^14.7.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "prettier": "^3.1.1"
  }
}
